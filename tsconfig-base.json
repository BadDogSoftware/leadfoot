{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"declaration": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"noUnusedParameters": false,
		"outDir": "./_build",
		"preserveConstEnums": true,
		"removeComments": false,
		"rootDir": ".",
		"sourceMap": true,
		"target": "ES5"
	}
}
